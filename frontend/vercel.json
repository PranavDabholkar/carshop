{
  "framework": "vite",
  "buildCommand": "chmod +x vercel-build.sh && ./vercel-build.sh",
  "outputDirectory": "dist",
  "installCommand": "npm install --force",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-api-url.vercel.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
